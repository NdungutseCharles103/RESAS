{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore .",
    "format": "prettier --write --ignore-path .gitignore \"**/*.{js,ts,vue,json,md}\"",
    "lint:fix": "eslint --ext .ts,.vue --ignore-path .gitignore . --fix",
    "test": "vitest --run --reporter verbose --globals",
    "test:watch": "vitest --reporter verbose --globals"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@types/node": "^18.16.19",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.44.0",
    "eslint-plugin-vue": "^9.15.1",
    "nuxt": "^3.6.2",
    "prettier": "^3.0.0"
  },
  "dependencies": {
    "@nuxt/test-utils": "^3.6.2",
    "chart.js": "^4.3.0",
    "vitest": "^0.33.0",
    "vue-chartjs": "^5.2.0"
  }
}
